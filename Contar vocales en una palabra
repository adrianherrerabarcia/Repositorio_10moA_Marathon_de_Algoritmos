#include <iostream>
#include <cstring>
using namespace std;

int main() {
    char palabra[100];
    int vocales = 0;
    cout << "Ingrese una palabra: ";
    cin >> palabra;
    for (int i = 0; palabra[i] != '\0'; i++) {
        char c = tolower(palabra[i]);
        if (c=='a'||c=='e'||c=='i'||c=='o'||c=='u') vocales++;
    }
    cout << "Numero de vocales: " << vocales;
}
