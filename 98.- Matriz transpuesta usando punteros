#include <iostream>
using namespace std;

int main() {
    int n, m;
    cin >> n >> m;
    int **mat = new int*[n];
    for (int i = 0; i < n; i++) mat[i] = new int[m];

    for (int i = 0; i < n; i++)
        for (int j = 0; j < m; j++)
            cin >> mat[i][j];

    int **trans = new int*[m];
    for (int i = 0; i < m; i++) trans[i] = new int[n];

    for (int i = 0; i < n; i++)
        for (int j = 0; j < m; j++)
            trans[j][i] = mat[i][j];

    for (int i = 0; i < m; i++) {
        for (int j = 0; j < n; j++) cout << trans[i][j] << " ";
        cout << endl;
    }
}
