#include <iostream>
#include <vector>
using namespace std;

long long fibo(int n, vector<long long>& memo) {
    if (n <= 1) return n;
    if (memo[n] != -1) return memo[n];
    return memo[n] = fibo(n-1, memo) + fibo(n-2, memo);
}

int main() {
    int n;
    cout << "n: "; cin >> n;
    vector<long long> memo(n+1, -1);
    cout << fibo(n, memo);
}
