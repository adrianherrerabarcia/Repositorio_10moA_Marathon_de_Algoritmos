#include <iostream>
using namespace std;
int main(){
    int m[3][3];
    cout<<"Matriz 3x3:\n";
    for(int i=0;i<3;i++)
        for(int j=0;j<3;j++) cin>>m[i][j];
    cout<<"Transpuesta:\n";
    for(int i=0;i<3;i++){
        for(int j=0;j<3;j++) cout<<m[j][i]<<" ";
        cout<<endl;
    }
    return 0;
}
