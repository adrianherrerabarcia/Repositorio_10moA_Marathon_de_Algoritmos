#include <iostream>
using namespace std;

int main() {
    int n, par = 0, impar = 0;
    cout << "Cantidad de numeros: ";
    cin >> n;
    int arr[n];
    for (int i = 0; i < n; i++) cin >> arr[i];
    for (int i = 0; i < n; i++) {
        if (arr[i] % 2 == 0) par += arr[i];
        else impar += arr[i];
    }
    cout << "Suma pares: " << par << "\nSuma impares: " << impar;
}
