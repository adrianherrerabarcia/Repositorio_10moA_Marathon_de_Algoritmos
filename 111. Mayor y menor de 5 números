#include <iostream>
using namespace std;
int main(){
    int a[5],may,men;
    for(int i=0;i<5;i++){
        cout<<"Numero "<<i+1<<": "; cin>>a[i];
    }
    may=men=a[0];
    for(int i=1;i<5;i++){
        if(a[i]>may) may=a[i];
        if(a[i]<men) men=a[i];
    }
    cout<<"Mayor: "<<may<<", Menor: "<<men;
    return 0;
}
