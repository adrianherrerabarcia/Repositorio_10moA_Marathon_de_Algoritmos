#include <iostream>
using namespace std;

int main(){
    int d1,m1,a1,d2,m2,a2;
    cout<<"Primera fecha (d m a): ";
    cin>>d1>>m1>>a1;
    cout<<"Segunda fecha (d m a): ";
    cin>>d2>>m2>>a2;
    int dias = (a2-a1)*365 + (m2-m1)*30 + (d2-d1);
    cout<<"Diferencia aproximada: "<<dias<<" dias";
}
