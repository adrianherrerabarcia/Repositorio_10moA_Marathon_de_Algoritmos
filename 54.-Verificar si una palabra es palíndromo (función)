#include <iostream>
#include <string>
using namespace std;

bool esPalindromo(string s) {
    int i = 0, j = s.size() - 1;
    while (i < j) {
        if (tolower(s[i]) != tolower(s[j])) return false;
        i++; j--;
    }
    return true;
}

int main() {
    string palabra;
    cin >> palabra;
    cout << (esPalindromo(palabra) ? "Si" : "No");
}
