#include <iostream>
using namespace std;

#define N 5
int mejor = 999999;

void buscar(int mat[N][N], int x, int y, int suma) {
    if (x == N - 1 && y == N - 1) {
        mejor = min(mejor, suma + mat[x][y]);
        return;
    }
    if (x >= N || y >= N) return;

    buscar(mat, x + 1, y, suma + mat[x][y]);
    buscar(mat, x, y + 1, suma + mat[x][y]);
}

int main() {
    int mat[N][N] = {{1,3,1,2,9},{2,1,8,1,2},{4,2,1,3,3},{5,2,1,8,1},{1,1,1,1,1}};
    buscar(mat, 0, 0, 0);
    cout << "Camino minimo: " << mejor;
}

