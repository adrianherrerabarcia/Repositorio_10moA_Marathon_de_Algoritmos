#include <iostream>
#include <string>
using namespace std;

bool palindromo(const string& s, int i, int j) {
    if (i >= j) return true;
    if (s[i] != s[j]) return false;
    return palindromo(s, i + 1, j - 1);
}

int main() {
    string s;
    cin >> s;
    cout << (palindromo(s, 0, s.size() - 1) ? "Es palindromo" : "No es palindromo");
}
