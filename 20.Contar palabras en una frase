#include <iostream>
#include <string>
using namespace std;

int main() {
    string frase;
    cout << "Ingrese una frase: ";
    getline(cin, frase);
    int palabras = 0;
    for (int i = 0; i < frase.size(); i++)
        if (frase[i] == ' ') palabras++;
    cout << "Cantidad de palabras: " << palabras + 1;
}
