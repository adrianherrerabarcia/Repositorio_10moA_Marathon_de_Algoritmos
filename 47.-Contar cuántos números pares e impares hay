#include <iostream>
using namespace std;

int main() {
    int n, pares = 0, impares = 0;
    cin >> n;
    int arr[n];
    for (int i = 0; i < n; i++) cin >> arr[i];
    for (int i = 0; i < n; i++) {
        if (arr[i] % 2 == 0) pares++;
        else impares++;
    }
    cout << "Pares: " << pares << "\nImpares: " << impares;
}
