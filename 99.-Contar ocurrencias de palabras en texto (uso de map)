20. Sudoku Solver (Backtracking)
#include <iostream>
using namespace std;

bool valido(int grid[9][9], int fila, int col, int num) {
    for (int x = 0; x < 9; x++)
        if (grid[fila][x] == num || grid[x][col] == num)
            return false;

    int startRow = fila - fila % 3, startCol = col - col % 3;
    for (int i = 0; i < 3; i++)
        for (int j = 0; j < 3; j++)
            if (grid[i + startRow][j + startCol] == num)
                return false;
    return true;
}

