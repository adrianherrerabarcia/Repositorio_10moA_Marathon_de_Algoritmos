#include <iostream>
#include <fstream>
#include <string>
using namespace std;

int main() {
    ofstream out("texto.txt");
    out << "Hola Adrian\nEsto es un archivo de prueba.";
    out.close();

    ifstream in("texto.txt");
    string linea;
    while (getline(in, linea))
        cout << linea << endl;
    in.close();
}
