#include <iostream>
#include <cmath>
using namespace std;

bool esPrimo(int n) {
    if (n < 2) return false;
    for (int i = 2; i <= sqrt(n); i++)
        if (n % i == 0) return false;
    return true;
}

int main() {
    int n;
    cout << "Ingrese un numero: ";
    cin >> n;
    for (int i = 2; i < n - 2; i++) {
        if (esPrimo(i) && esPrimo(i + 2))
            cout << "(" << i << ", " << i + 2 << ")\n";
    }
}
